{
  "service": "canary-deployments-test",
  "custom": {
    "deploymentSettings": {
      "stages": [
        "dev"
      ]
    }
  },
  "functions": {
    "bello": {
      "handler": "handler.bello",
      "events": [
        {
          "schedule": {
            "name": "test-scheduled-event",
            "rate": "rate(1 minute)",
            "enabled": true
          }
        }
      ],
      "deploymentSettings": {
        "type": "Linear10PercentEvery1Minute",
        "alias": "Live",
        "preTrafficHook": "preHook",
        "postTrafficHook": "postHook",
        "alarms": [
          "BelloFooAlarm"
        ]
      }
    },
    "preHook": {
      "handler": "hooks.pre"
    },
    "postHook": {
      "handler": "hooks.post"
    }
  }
}
